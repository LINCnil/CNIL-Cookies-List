{
  "browser_action": {
    "browser_style": true,
    "default_title": "CNIL - Cookies List",
    "default_icon": {      
		"96": "icons/icon96.png",
		"200": "icons/icon200.png"
    }
  },
  
	"developer": 
	{
		"name": "CNIL - Service des contrôles"
	},

  "description": "Liste les cookies enregistrés dans le navigateur.",
  "icons": {
	"200": "icons/logo200.png"
  },
  "manifest_version": 2,
  "name": "CNIL - Cookies List",
  "version": "1.0.0",
  "permissions": ["cookies","<all_urls>","tabs"],
  
   "background": {"scripts": ["start.js"]}
}